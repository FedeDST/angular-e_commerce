<?xml version="1.0" encoding="UTF-8" ?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file source-language="it" target-language="en" datatype="plaintext" original="ng2.template">
    <body>

      <trans-unit id="itemTitle" datatype="html">
        <source><x id="INTERPOLATION" equiv-text="{{ p.title }}"/></source>
        <target><x id="INTERPOLATION" equiv-text="{{ p.title }}"/></target>
      </trans-unit>

      <trans-unit id="itemQuantity" datatype="html">
        <source>Quantità x <x id="INTERPOLATION" equiv-text="{{p.quantity}}"/></source>
        <target>Quantity x <x id="INTERPOLATION" equiv-text="{{p.quantity}}"/></target>
      </trans-unit>

      <trans-unit id="partial" datatype="html">
        <source>Totale Parziale : $<x id="INTERPOLATION" equiv-text="{{ p.price * p.quantity }}"/></source>
        <target>Subtotal: $<x id="INTERPOLATION" equiv-text="{{ p.price * p.quantity }}"/></target>
      </trans-unit>

      <trans-unit id="cartClear" datatype="html">
        <source> Svuota carrello </source>
        <target> Clear cart </target>
      </trans-unit>

      <trans-unit id="total" datatype="html">
        <source>
          Totale : $<x id="INTERPOLATION" equiv-text="{{total$ | async}}"/>
          <x id="START_BLOCK_IF" equiv-text="@if(isDetail &amp;&amp; actualShippingCost() &gt; 0){"/>
          <x id="START_SMALL_TEXT" ctype="x-small" equiv-text="&lt;small&gt;"/>
          di cui $<x id="INTERPOLATION_1" equiv-text="{{actualShippingCost()}}"/> di spedizione.
          <x id="CLOSE_SMALL_TEXT" ctype="x-small" equiv-text="&lt;/small&gt;"/>
          <x id="CLOSE_BLOCK_IF" equiv-text="}"/>
        </source>
        <target>
          Total: $<x id="INTERPOLATION" equiv-text="{{total$ | async}}"/>
          <x id="START_BLOCK_IF" equiv-text="@if(isDetail &amp;&amp; actualShippingCost() &gt; 0){"/>
          <x id="START_SMALL_TEXT" ctype="x-small" equiv-text="&lt;small&gt;"/>
          including $<x id="INTERPOLATION_1" equiv-text="{{actualShippingCost()}}"/> shipping.
          <x id="CLOSE_SMALL_TEXT" ctype="x-small" equiv-text="&lt;/small&gt;"/>
          <x id="CLOSE_BLOCK_IF" equiv-text="}"/>
        </target>
      </trans-unit>

      <trans-unit id="goToCheckout" datatype="html">
        <source> Vai al checkout.</source>
        <target> Go to checkout.</target>
      </trans-unit>

      <trans-unit id="4983379385161270565">
        <source>Prodotto Rimosso</source>
        <target>Product removed</target>
      </trans-unit>

      <trans-unit id="7573353348625505353">
        <source>Prodotto aggiunto</source>
        <target>Product added</target>
      </trans-unit>

      <trans-unit id="3305710880308394473">
        <source>Carrello svuotato</source>
        <target>Cart cleared</target>
      </trans-unit>

      <trans-unit id="step1Title">
        <source>Dettagli Cliente</source>
        <target>Customer details</target>
      </trans-unit>

      <trans-unit id="step1SubTitle">
        <source>Inserisci i tuoi dati per procedere con l&apos;ordine.</source>
        <target>Enter your details to proceed with the order.</target>
      </trans-unit>

      <trans-unit id="step1Name">
        <source>Nome</source>
        <target>First name</target>
      </trans-unit>

      <trans-unit id="step1NamePlaceholder">
        <source>Inserisci il nome</source>
        <target>Enter first name</target>
      </trans-unit>

      <trans-unit id="step1InvalidName">
        <source>Il nome è un campo obbligatorio e deve essere lungo almeno 3 caratteri.</source>
        <target>First name is required and must be at least 3 characters long.</target>
      </trans-unit>

      <trans-unit id="step1Surname">
        <source>Cognome</source>
        <target>Last name</target>
      </trans-unit>

      <trans-unit id="step1SurnamePlaceholder">
        <source>Inserisci il cognome</source>
        <target>Enter last name</target>
      </trans-unit>

      <trans-unit id="step1InvalidSurname">
        <source>Il cognome è un campo obbligatorio e deve essere lungo almeno 3 caratteri.</source>
        <target>Last name is required and must be at least 3 characters long.</target>
      </trans-unit>

      <trans-unit id="step1Email">
        <source>Email</source>
        <target>Email</target>
      </trans-unit>

      <trans-unit id="step1EmailPlaceholder">
        <source>Inserisci l&apos;email</source>
        <target>Enter email</target>
      </trans-unit>

      <trans-unit id="step1InvalidEmail">
        <source>L&apos;email è un campo obbligatorio e deve avere il formato corretto.</source>
        <target>Email is required and must be valid.</target>
      </trans-unit>

      <trans-unit id="step1Address">
        <source>Indirizzo</source>
        <target>Address</target>
      </trans-unit>

      <trans-unit id="step1AddressPlaceholder">
        <source>Es.Viale Europa 30 00100 RM</source>
        <target>e.g. Viale Europa 30 00100 RM</target>
      </trans-unit>

      <trans-unit id="step1InvalidAddress">
        <source>L&apos;indirizzo è un campo obbligatorio e deve essere lungo almeno 5 caratteri.</source>
        <target>Address is required and must be at least 5 characters long.</target>
      </trans-unit>

      <trans-unit id="step1Continue">
        <source> Continua </source>
        <target> Continue </target>
      </trans-unit>

      <trans-unit id="step2Title">
        <source>Seleziona il metodo di spedizione:</source>
        <target>Select shipping method:</target>
      </trans-unit>

      <trans-unit id="step2Back">
        <source>Indietro</source>
        <target>Back</target>
      </trans-unit>

      <trans-unit id="step2Next">
        <source>Avanti</source>
        <target>Next</target>
      </trans-unit>

      <trans-unit id="step3Title">
        <source>Dettagli di pagamento</source>
        <target>Payment details</target>
      </trans-unit>

      <trans-unit id="step3CardName">
        <source>Nome della carta</source>
        <target>Cardholder name</target>
      </trans-unit>

      <trans-unit id="step3CardNamePlaceholder">
        <source>Inserisci il nome della carta</source>
        <target>Enter cardholder name</target>
      </trans-unit>

      <trans-unit id="step3InvalidCardName">
        <source>Il nome della carta è un campo obbligatorio e deve essere lungo almeno 4 caratteri.</source>
        <target>Cardholder name is required and must be at least 4 characters long.</target>
      </trans-unit>

      <trans-unit id="step3CardNumber">
        <source>Numero della carta</source>
        <target>Card number</target>
      </trans-unit>

      <trans-unit id="step3CardNumberPlaceholder">
        <source>Inserisci il numero della carta</source>
        <target>Enter card number</target>
      </trans-unit>

      <trans-unit id="step3InvalidCardNumber">
        <source>Il nome della carta è un campo obbligatorio e deve essere lungo esattamente 16 caratteri.</source>
        <target>Card number is required and must be exactly 16 characters long.</target>
      </trans-unit>

      <trans-unit id="step3Back">
        <source>Indietro</source>
        <target>Back</target>
      </trans-unit>

      <trans-unit id="step3Next">
        <source>Avanti</source>
        <target>Next</target>
      </trans-unit>

      <trans-unit id="step4Title">
        <source>Riepilogo Ordine</source>
        <target>Order summary</target>
      </trans-unit>

      <trans-unit id="step4ConfirmOrder">
        <source> Conferma Ordine</source>
        <target> Confirm order</target>
      </trans-unit>

      <trans-unit id="step4Back">
        <source> Indietro</source>
        <target> Back</target>
      </trans-unit>

      <trans-unit id="step4Reset">
        <source> Ritorna alla selezione dei prodotti</source>
        <target> Back to product selection</target>
      </trans-unit>

      <trans-unit id="7651394492551077474">
        <source>Acquisto effettuato</source>
        <target>Purchase completed</target>
      </trans-unit>

    </body>
  </file>
</xliff>
